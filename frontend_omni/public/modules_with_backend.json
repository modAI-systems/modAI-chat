{
  "version": "1.0.0",
  "modules": [
    {
      "id": "authentication-service",
      "type": "GlobalContextProvider",
      "path": "@/modules/authentication-service/AuthContextProvider"
    },
    {
      "id": "authentication-router",
      "type": "RouterEntry",
      "path": "@/modules/authentication/AuthRouterEntry",
      "dependencies": ["module:authentication-service", "flag:!sessionActive"]
    },
    {
      "id": "authentication-sidebar-footer",
      "type": "SidebarFooterItem",
      "path": "@/modules/authentication/AuthSidebarFooterItem",
      "dependencies": ["module:authentication-service"]
    },
    {
      "id": "session",
      "type": "GlobalContextProvider",
      "path": "@/modules/session-provider/SessionContextProvider"
    },
    {
      "id": "user-service",
      "type": "GlobalContextProvider",
      "path": "@/modules/user-service/UserServiceContextProvider",
      "dependencies": ["module:session"]
    },
    {
      "id": "chat-layout-sidebar",
      "type": "SidebarItem",
      "path": "@/modules/chat-layout/ChatSidebarItem",
      "dependencies": ["module:session", "flag:sessionActive"]
    },
    {
      "id": "chat-layout-router",
      "type": "RouterEntry",
      "path": "@/modules/chat-layout/ChatRouterEntry",
      "dependencies": ["module:session", "flag:sessionActive"]
    },
    {
      "id": "chat-messages-area-pane",
      "type": "ChatMiddlePane",
      "path": "@/modules/chat-layout/ChatArea",
      "dependencies": ["flag:sessionActive"]
    },
    {
      "id": "llm-context",
      "type": "GlobalContextProvider",
      "path": "@/modules/llm-picker/LLMContextProvider"
    },
    {
      "id": "llm-chat-top-pane",
      "type": "ChatTopPane",
      "path": "@/modules/llm-picker/ChatTopPane"
    },
    {
      "id": "llm-provider-service",
      "type": "GlobalContextProvider",
      "path": "@/modules/llm-provider-service/LLMNoBackendProviderService"
    },
    {
      "id": "chat-side-panels-provider",
      "type": "GlobalContextProvider",
      "path": "@/modules/chat-layout/ChatSidePanelProvider"
    },
    {
      "id": "global-settings-sidebar-footer",
      "type": "SidebarFooterItem",
      "path": "@/modules/settings/SidebarFooterItemGlobal"
    },
    {
      "id": "global-settings-router",
      "type": "RouterEntry",
      "path": "@/modules/settings/RouterEntryGlobal"
    }
  ]
}
